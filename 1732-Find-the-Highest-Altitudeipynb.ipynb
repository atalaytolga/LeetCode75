    "class Solution:\n",
    "    def largestAltitude(self, gain: List[int]) -> int:\n",
    "        maxAlt = 0\n",
    "        curAlt = 0\n",
    "        for n in gain:\n",
    "            curAlt += n\n",
    "            maxAlt = max(maxAlt, curAlt)\n",
    "        return maxAlt"
   ]
  }
