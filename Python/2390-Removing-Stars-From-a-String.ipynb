   "source": [
    "class Solution:\n",
    "    def removeStars(self, s: str) -> str:\n",
    "        stack = []\n",
    "        for n in s:\n",
    "            if n == '*':\n",
    "                stack.pop()\n",
    "            else:\n",
    "                stack.append(n)\n",
    "        return ''.join(stack)\n",
    "                "
   ]
  }
