    "class RecentCounter:\n",
    "    def __init__(self):\n",
    "        self.counter = []\n",
    "\n",
    "    def ping(self, t: int) -> int:\n",
    "        self.counter.append(t)\n",
    "        while self.counter[-1] - self.counter[0] > 3000:\n",
    "            self.counter.pop(0)\n",
    "        return len(self.counter)"
   ]
