    "class Solution:\n",
    "    def tribonacci(self, n: int) -> int:\n",
    "        res = [0, 1, 1]\n",
    "        if n < 3:\n",
    "            return res[n]\n",
    "        \n",
    "        for i in range(n - 2):\n",
    "            res = res[1:] + [sum(res)]\n",
    "        return res[-1]"
   ]
